    "@types/node": "^18.6.3",
    "archiver": "^5.3.1",
    "aws-cdk-lib": "^2.39.1",
    "commander": "^9.4.0",
    "constructs": "^10.1.92",
    "replace-in-file": "^6.3.5",
    "serverless-http": "^3.0.2",
    "simple-git": "^3.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@tsconfig/node18": "^1.0.1",
    "adm-zip": "^0.5.9",
