{
  "name": "@sladg/nextjs-lambda",
  "version": "3.8.7",
  "description": "Plug-and-play lambda for replacing default NextJS image optimization handler.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sladg/nextjs-lambda"
  },
  "license": "MIT",
  "author": "Jan Soukup <jan@ssoukup.com>",
  "exports": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "bin": {
    "next-utils": "./dist/cli.js"
  },
  "files": [
    "dist",
    "scripts",
    "cdk"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run build:main && npm run build:sharp && npm run build:next",
    "build:main": "rollup -c",
    "build:next": "./scripts/build-next-layer.sh",
    "build:sharp": "./scripts/build-sharp-layer.sh",
    "cdk:deploy": "cdk deploy",
    "release": "node ./dist/cli.js shipit --gitUser @sladg --gitEmail jan@ssoukup.com"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.41.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.41.0-alpha.0",
    "@aws-sdk/client-s3": "^3.181.0",
    "@types/archiver": "^5.3.1",
    "@types/aws-lambda": "^8.10.103",
    "@types/node": "^18.7.16",
    "archiver": "^5.3.1",
    "aws-cdk": "^2.44.0",
    "aws-cdk-lib": "^2.44.0",
    "commander": "^9.4.0",
    "constructs": "^10",
    "next": "^12.3.0",
    "replace-in-file": "^6.3.5",
    "serverless-http": "^3.0.2",
    "simple-git": "^3.14.1"
  },
  "devDependencies": {
    "adm-zip": "^0.5.9",
    "critters": "^0.0.16",
    "esbuild": "^0.15.7",
    "rollup": "^2.79.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=16"
  },
  "engineStrict": true
}
