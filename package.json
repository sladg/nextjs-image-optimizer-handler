{
  "name": "@sladg/nextjs-lambda",
  "version": "3.1.2",
  "description": "Plug-and-play lambda for replacing default NextJS image optimization handler.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sladg/nextjs-lambda"
  },
  "license": "MIT",
  "author": "Jan Soukup <jan@ssoukup.com>",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "bin": {
    "next-utils": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run build:main && npm run build:sharp && npm run build:next",
    "build:main": "rollup -c",
    "build:next": "./scripts/build-next-layer.sh",
    "build:sharp": "./scripts/build-sharp-layer.sh",
    "publish": "npm publish",
    "release": "node ./dist/cli.js shipit"
  },
  "dependencies": {
    "@types/archiver": "^5.3.1",
    "@types/aws-lambda": "^8.10.101",
    "@types/node": "^18.6.3",
    "archiver": "^5.3.1",
    "commander": "^9.4.0",
    "replace-in-file": "^6.3.5",
    "serverless-http": "^3.0.2",
    "simple-git": "^3.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@tsconfig/node18": "^1.0.1",
    "adm-zip": "^0.5.9",
    "critters": "^0.0.16",
    "esbuild": "^0.15.6",
    "rollup": "^2.79.0",
    "rollup-plugin-ts": "^3.0.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.39.1-alpha",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.39.1-alpha",
    "@aws-sdk/client-s3": "^3.142",
    "aws-cdk-lib": "^2.39",
    "constructs": "^10",
    "next": "^12",
    "typescript": "^4.8"
  },
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
